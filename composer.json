{
    "name": "mmorinaga/cakephp-base-model-baker",
    "description": "BaseModelBaker plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "cakephp/cakephp": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "BaseModelBaker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BaseModelBaker\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
